@if(ViewBag.Mensaje != null)
{
    <div>
        <p class="text-info">@ViewBag.Mensaje</p>
    </div>

}



<div class="text-center">
    <div class="container">
        <div class="row">

            <div class="col-lg-4"></div>

            <div class="col-lg-4 col-md-12">
                <div class="fadeImg1">
                    <h1 class="display-5" style="color:navy"> ORT <br> Salud </h1>
                </div>

                <div class="fadeImg2">
                    <p> Bienvenido al Portal de Salud del grupo ORT-Develop </p>
                </div>
                <div class="fadeImg3">
                    <div class="imagen1">
                        <img src="data:image/png;base64," />
                    </div>
                </div>
            </div>

            <div class="col-lg-4 "></div>
        </div>
        <div class="row">
            <div class="col-lg-4"></div>
            <div class="col-lg-4 col-md-12">
                <div class="fadeImg4">
                    @if (User.Identity.IsAuthenticated)
                    {
                        if (User.IsInRole("Empleado"))
                        {
                            <a class="boton" asp-area="" asp-controller="Empleados" asp-action="MenuEmpleado">Ingresar</a>
                        }
                        if (User.IsInRole("Medico"))
                         {
                            <a class="boton" asp-area="" asp-controller="Medicos" asp-action="MenuMedico">Ingresar</a>
                        }
                        else if (User.IsInRole("Paciente"))
                          {
                            <a class="boton" asp-area="" asp-controller="Pacientes" asp-action="MenuPaciente">Ingresar</a>

                          }


                    }
                    else
                    {
                        <a class="boton" asp-area="" asp-controller="Account" asp-action="IniciarSesion">Ingresar</a>
                    }
                   
                </div>
            </div>
            <div class="col-lg-4"></div>
        </div>
    </div>
</div>
<style type="text/css">
        body {
            font-family: Verdana, Arial;
            background-repeat: no-repeat;
            background-color: #ffffff;
            background-position: center;
            background-size: 200px, 200px;
            margin: 50px;
        }

                h1 {
                    text-align: center
                }
        .ingresar {
        }

                .fadeImg1 {
                    animation: fadeIn 2s;
                }
                .fadeImg2 {
                    animation: fadeIn 3s;
                }
                .fadeImg3 {
                    animation: fadeIn 4s;
                }
                .fadeImg4 {
                    animation: fadeIn 5s;
                }

    .imagen1 {
        position: relative;
        animation-name: movimiento;
        animation-duration: 3s;
    }
       @@keyframes fadeIn {
             0% { opacity: 0; }
            100% { opacity: 1; }
                    }


       @@keyframes movimiento {
            from {left:800px;}
            to {left: 0px;
                            }
}
        form {
            padding: 10px;
            border-style: solid;
            width: 250px;
        }
</style>

</div>
