@model Medico

<div>
    <form method="post">
        <div>
            <label> Nombre: </label>
            <input type="text" name="nombre" />
        </div>
        <div>
            <label> Segundo Nombre: </label>
            <input type="text" name="segundo nombre" />
        </div>
        <div>
            <label> Apellido: </label>
            <input type="text" name="apellido" />
        </div>
        <div>
            <label> DNI: </label>
            <input type="text" name="dni" />
        </div>
        <div>
            <label> Email: </label>
            <input type="email" name="email" />
        </div>
        <div>
            <label> Telefono: </label>
            <input type="tel" name="telefono" />
        </div>
        <div>
            <label> Fecha De Alta: </label>
            <input type="date" name="fecha alta" />
        </div>
        <div>
            <label> Usuario: </label>
            <input type="text" name="usuario" />
        </div>
        <div>
            <label> Matricula Nacional: </label>
            <input type="number" name="matricula nacional" />
        </div>
        <div>
            <label> Matricula Provincial: </label>
            <input type="number" name="matricula provincial" />
        </div>
        <div>
            <label> Especialidades: </label>
            <input type="checkbox" name="especialidades" />
        </div>
        <div>
            <label> Esta Activo: </label>
            <input type="checkbox" name="nombre" />
        </div>
        <div>
            <input type="submit" value="Dar de Alta" />
        </div>

    </form>


</div>
