@model Historias_Clinicas.Models.Evolucion



<div class="container">
    <div class="row">
        <div class=" col-lg-2"></div>
        <div class=" col-lg-8 col-md-12">
            <h1> Esta seguro que desea Cerrar este Evolucion?</h1>

            <h1> Esta accion es irreverisble</h1>



            <a class="boton" asp-action="CerrarEvolucion" asp-route-id="@TempData["EpisodioId"]" asp-route-paciente="@TempData["paciente"]">Cerrar Evolucion</a>
            <a type="button" class="btn btn-secondary" style="margin-left: 20px" asp-action="Delete" asp-route-id="">Volver</a>
        </div>
    </div>
    <div class="col-lg-2"></div>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
