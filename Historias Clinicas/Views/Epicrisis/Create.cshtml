@model Historias_Clinicas.Models.Epicrisis

@{
    ViewData["Title"] = "Create";
}

<h1>Crear</h1>

<h4>Epicrisis</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
        <div class="form-group">
                <input class="boton" type="submit" asp-route-id="@ViewData["EpisodioId"]" value="Crear"/>
            <a type="button" class="btn btn-secondary" style="margin-left: 20px" asp-action="HistoriaClincaDePaciente" asp-controller="HistoriaClincas" asp-route-id="@TempData["pacienteId"]">Cancelar</a>
        </div>
        </form>
    </div>
</div>


@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
